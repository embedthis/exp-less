{
    "name": "exp-less",
    "description": "Lessc Plugin for Expansive",
    "version": "0.2.2",
    "keywords": [
        "expansive-plugin",
        "less"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-less.git"
    },
    "scripts": {
        "postcache": {
            "script": "if (!Cmd.locate('lessc')) { if (!Cmd.locate('npm')) { throw 'Cannot locate the \"npm\" utility.' } else { run('npm install -g less')}}"
        }
    },
    "pak": {
        "version": ">=0.10.0"
    }
}
